@prefix sh: <http://www.w3.org/ns/shacl#> .
#@prefix shr: <http://www.w3.org/ns/shacl/repairs#> .
@prefix : <http://org.shacl.repairs.tests/> .

:ViolatedContractStrategy
    a sh:RepairStrategy ;
    sh:repair [
        sh:path :hasStates ;
        sh:action sh:delete ;
        sh:not [
        # sh:value is a concrete value to (not) delete; or a value generated by a strategy, like min or max
            sh:value :stateViolated ;
        ] ;
    ] ;
    sh:repair [
        sh:path :hasTerms ;
        sh:action sh:delete ;
        sh:not [
        # sh:value is a concrete value to (not) delete; or a value generated by a strategy, like min or max
            sh:value [
                a sh:AnyValue ;
            ]
        ] ;
    ] ;
    sh:repair [
        sh:path :hasObligations ;
        sh:action sh:delete ;
        sh:not [
            sh:value [
                a sh:AnyValue ;
            ]
        ] ;
    ] .